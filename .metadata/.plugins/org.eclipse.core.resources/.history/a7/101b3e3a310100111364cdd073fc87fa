package arrays;

import java.util.Arrays;
import java.util.Iterator;

public class Pex√°men1 {

	final static int MAX = 3;
	final static int COL = 6;
	public static void mostrar(int[] arr) {
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i]+" ");
		}
	}
	
	public static int contaN(int[] arr,int N) {
		int conta =0;
		for (int i = 0; i < arr.length; i++) {
			if(arr[i]==N)
				conta++;
		}
		return conta;
	}
	public static void moverNalFinal(int[] arr,int pos) {
		int aux;
		for (int i = pos; i < arr.length-1; i++) {
			aux=arr[i+1];
			arr[i+1]=arr[i];
			arr[i]=aux;
			
		}
	}
	
	public static void OrdinoBinario(int[] numsArr ) {
		int aux;
		boolean cambio;
		for (int i = 0; i < numsArr.length -1; i++) {
			cambio=false;
			for (int j = 0; j < numsArr.length -1; j++) {
				if (numsArr[j+1]<numsArr[j]) {
					aux=numsArr[j+1];
					numsArr[j+1]=numsArr[j];
					numsArr[j]=aux;
					cambio=true;
				}
				
			}
			if(!cambio)
				break;	
		}
	}
	public static boolean encuentra(int arr[],int N) {
		for (int i = 0; i < arr.length; i++) {
			if(arr[i]==N)
				return true;
		}
		return false;
	}


	public static void main(String[] args) {
		int[][] nums = {{3,2,3,4,5,6},//2 ,2
						{9,2,3,6,5,8},
						{2,1,3,4,5,6},
						};
		int[] numsArr = {3,4,4,8,8,7,6,6,6};
	
		
		for (int i = 0; i < numsArr.length; i++) {
			if (!encuentra(numsArr, numsArr[i])) {
				System.out.print(numsArr[i]+": "+contaN(numsArr,numsArr[i]));
				System.out.println();
			}
			
		}
		
		//mostrar(numsArr);
	}

}
