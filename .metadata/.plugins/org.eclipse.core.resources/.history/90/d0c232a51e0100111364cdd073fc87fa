package arrays;

import java.util.Iterator;

public class Pex√°men1 {

	final static int MAX = 3;
	final static int COL = 6;
	public static void mostrar(int[] arr) {
		for (int i = 0; i < arr.length; i++) {
			System.out.print(arr[i]+" ");
		}
	}
	
	public static void moverNalFinal(int[] arr,int pos) {
		int aux;
		for (int i = pos; i < arr.length-1; i++) {
			aux=arr[i+1];
			arr[i+1]=arr[i];
			arr[i]=aux;
			
		}
	}
	public static void main(String[] args) {
		int[][] nums = {{3,2,3,4,5,6},//2 ,2
						{9,2,3,6,5,8},
						{2,1,3,4,5,6},
						};
		int[] numsArr = {3,0,0,4,5,6};
		
		
		for (int i = 0; i < numsArr.length; i++) {
			if (numsArr[i] ==0) {
				moverNalFinal(numsArr, numsArr[i]);
			}
		}
		
		
		mostrar(numsArr);
	}

}
